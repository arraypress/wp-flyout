{
  "name": "arraypress/wp-flyout",
  "description": "Modern, accessible flyout panels for WordPress admin interfaces with tabbed navigation and reusable UI components",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-flyout/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "arraypress/wp-composer-assets": "dev-main",
    "arraypress/wp-ajax-select": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WPFlyout\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-flyout/issues",
    "source": "https://github.com/arraypress/wp-flyout"
  },
  "keywords": [
    "wordpress",
    "flyout",
    "modal",
    "panel",
    "admin",
    "ui-components",
    "tabs",
    "form-builder"
  ]
}